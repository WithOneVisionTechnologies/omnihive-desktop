{
    "name": "omnihive-desktop",
    "productName": "OmniHive Desktop",
    "version": "1.0.0",
    "description": "Desktop Manager for the OmniHive Ecosystem",
    "main": ".webpack/main",
    "scripts": {
        "start": "ELECTRON_DISABLE_SECURITY_WARNINGS=true electron-forge start --inspect-electron",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish"
    },
    "keywords": [],
    "author": {
        "name": "Doug Shontz",
        "email": "doug.shontz@withone.vision"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "src/assets/images/Application.icns"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "OmniHive Desktop"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/index.html",
                                    "js": "./src/renderer.ts",
                                    "name": "main_window"
                                }
                            ]
                        }
                    }
                ]
            ]
        }
    },
    "dependencies": {
        "@headlessui/react": "1.4.0",
        "@withonevision/omnihive-core": "6.2.61",
        "animate.css": "4.1.1",
        "electron-better-ipc": "2.0.1",
        "electron-is-dev": "2.0.0",
        "electron-squirrel-startup": "1.0.0",
        "is-ip": "3.1.0",
        "launchdarkly-react-client-sdk": "2.23.0",
        "lodash": "4.17.21",
        "rc-dock": "3.0.18",
        "rc-tree": "5.0.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-query": "3.21.0",
        "react-tooltip": "4.2.21",
        "socket.io-client": "4.1.3",
        "valtio": "1.2.1"
    },
    "devDependencies": {
        "@commitlint/cli": "13.1.0",
        "@commitlint/config-conventional": "13.1.0",
        "@electron-forge/cli": "6.0.0-beta.59",
        "@electron-forge/maker-deb": "6.0.0-beta.59",
        "@electron-forge/maker-rpm": "6.0.0-beta.59",
        "@electron-forge/maker-squirrel": "6.0.0-beta.59",
        "@electron-forge/maker-zip": "6.0.0-beta.59",
        "@electron-forge/plugin-webpack": "6.0.0-beta.59",
        "@types/lodash": "4.14.172",
        "@types/node": "16.7.2",
        "@types/react": "17.0.19",
        "@types/react-dom": "17.0.9",
        "@typescript-eslint/eslint-plugin": "4.29.3",
        "@typescript-eslint/parser": "4.29.3",
        "@vercel/webpack-asset-relocator-loader": "1.7.0",
        "autoprefixer": "10.3.3",
        "commitizen": "4.2.4",
        "copy-webpack-plugin": "9.0.1",
        "css-loader": "6.2.0",
        "cz-conventional-changelog": "3.3.0",
        "dotenv": "10.0.0",
        "dotenv-webpack": "7.0.3",
        "electron": "13.2.2",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.24.2",
        "eslint-plugin-prettier": "3.4.1",
        "file-loader": "6.2.0",
        "fork-ts-checker-webpack-plugin": "6.3.2",
        "node-loader": "2.0.0",
        "postcss": "8.3.6",
        "postcss-import": "14.0.2",
        "postcss-loader": "6.1.1",
        "postcss-nested": "5.0.6",
        "postcss-preset-env": "6.7.0",
        "postcss-url": "10.1.3",
        "prettier": "2.3.2",
        "standard-version": "9.3.1",
        "stylelint": "13.13.1",
        "stylelint-config-standard": "22.0.0",
        "style-loader": "3.2.1",
        "tailwindcss": "2.2.7",
        "ts-loader": "9.2.5",
        "typescript": "4.4.2",
        "webpack": "5.51.1"
    }
}
